import pandas as pd

# Path to the Excel file
excel_file = "/path/to/your/excel/file.xlsx"

# Read all sheets from the Excel file
excel_data = pd.read_excel(excel_file, sheet_name=None)

# Create a dictionary to store the dataframes
dataframes = {}

# Assign each sheet to a dataframe in the dictionary
for sheet_name, sheet_data in excel_data.items():
    dataframes[sheet_name] = sheet_data

# Access the dataframes using the sheet names as keys
# For example, to access the dataframe for sheet "Sheet1":
sheet1_dataframe = dataframes["Sheet1"]
